    <title>WeatherApp</title>
</head>
<body>


    <!-- HEADER -->
    <nav class=" header navbar navbar-light bg-dark">
        Weather App
       <a class="logout" href="/users/logout">Deconnexion</a>
   </nav> 

<div class="container">
<div class="row"> 
    <h2 class="topplus">Tickets disponibles pour le 20/8:</h2>
   <div class ="second-ligne">
    <%for(var i=0;i<travelFound.length;i++){%>
        <div class="row actu">
                <div class="etiquette col-4 justify-content-between">
                    <div class="col-1 text"><%=travelFound[i].departure%>/<%=travelFound[i].arrival%></div>  
                    <div class="col-2">Train de départ à: <%=travelFound[i].departureTime%></div>
                    <div class="col-2"><%=travelFound[i].price%>€</div>
                    <div class="col-1"><a href="/" class="btn btn-outline-danger">OK</a></div>
                </div>
        </div>
    <%}%>
    </div>
    </div>
</div>
</div>


    <script src="../javascripts/script.js"></script>
    <script src="https://code.jquery.com/jquery-3.3.1.slim.min.js" integrity="sha384-q8i/X+965DzO0rT7abK41JStQIAqVgRVzpbzo5smXKp4YfRvH+8abtTE1Pi6jizo" crossorigin="anonymous"></script>
